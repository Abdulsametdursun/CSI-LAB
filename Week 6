print("Houston strong".find("asd"))
print("Houston strong".replace("strong","powerful"))

print("X"*10)
for i in range(1,5):
  print("* "*i)

sentence = "We are in love with Python"
words =sentence.split()
print(words)
for word in words:
  print(word)

sentence = "We Houston are in love with Python Houston "
words = sentence.split()

#Counting to words
counter=0
for word in words:
  if(word=="Houston"):
    counter+=1
print(counter)
