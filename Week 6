test3="Kfdasfa"

def test_my_String(my_str):
  if(test2.startswith("A") or test2.startswith("B") or test2.startswith ("C")):
    print("OK")
  else:
    print("NOT OK")

test_my_String(test1)
test_my_String(test2)
