def thisIsMyFun(param1=9):
  print(param1)
thisIsMyFun()

def thisIsMyFun(param2,param1=9):
  print(param1, param2)
thisIsMyFun("asdf",78)

def thisIsMyFun(param2,param1=9):
  print(param2, param1)
thisIsMyFun("asdf",78)

