#Write a python function returns boolean value if input is palindrome

def is_palindrome(text):
  result=True
  reversed=""
  for i in range(len(text)-1,-1,-1):
    reversed+=text[i]
  if(reversed==text):
    result=False
  else:
    result=True
  
  return result

print(is_palindrome("madam"))
