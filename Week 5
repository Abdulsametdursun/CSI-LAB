#reversed it
text="Pyton rocks"
def reverseString(text):
  for letter in range(len(text)-1,-1,-1):
    print(text[letter],end="")

#return it
def reverseReturn(text):
  result=""
  for letter in range(len(text)-1,-1,-1):
    result=result+text[letter]

  return result
#"s" = ""+"s"
#"sk"="s"+"k"
